{
  "expo": {
    "name": "Cuban-Serbia Visa Center",
    "slug": "cuban-serbia-visa",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "cuban-serbia",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cubanserbia.visa"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0a1628"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.cubanserbia.visa"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "name": "Cuban-Serbia Visa Center",
      "shortName": "Cuban-Serbia",
      "description": "Gesti√≥n profesional de visados para cubanos que viajan a Serbia",
      "backgroundColor": "#0a1628",
      "themeColor": "#d4af37"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0a1628"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
